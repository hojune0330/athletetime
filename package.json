{
  "name": "athlete-time-community",
  "version": "2.0.0",
  "description": "Athlete Time - 육상 커뮤니티 백엔드 (PostgreSQL + Cloudinary + WebSocket)",
  "main": "server-postgres.js",
  "scripts": {
    "start": "node server-postgres.js",
    "start:legacy": "node community-server.js",
    "start:chat": "node chat-websocket-server.js",
    "start:all": "node server-postgres.js & node chat-websocket-server.js",
    "dev": "nodemon server-postgres.js",
    "dev:legacy": "nodemon community-server.js",
    "dev:chat": "nodemon chat-websocket-server.js",
    "db:migrate": "psql $DATABASE_URL < database/schema.sql",
    "db:seed": "node database/seed.js"
  },
  "keywords": [
    "athletics",
    "track-and-field",
    "community",
    "running",
    "websocket",
    "chat",
    "postgresql",
    "cloudinary"
  ],
  "author": "Athlete Time",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "sharp": "^0.33.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
